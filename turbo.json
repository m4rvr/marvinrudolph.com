{
  "$schema": "https://turbo.build/schema.json",
  "envMode": "loose",
  "ui": "tui",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["^dev"]
    },
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "deploy": {
      "cache": true,
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
